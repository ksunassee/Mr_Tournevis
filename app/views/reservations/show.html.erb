
<div class="container">

  <h1> Your reservation </h1>

  <% @reservations.each do |reservation| %>
    <p>
      <strong> Start-Date: </strong> 
      <%= reservation.name %> 
    </p>

    <p>
      <strong> Return-Date </strong> 
      <%= reservation.price %> 
    </p>
  <% end %>

  <%= link_to "Back to reservations", reservations_path, class: "btn btn-primary"%> 
</div>


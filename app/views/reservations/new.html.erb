<div class="container">
  <div class="form-wrapper">
    <h2>Book a Reservation</h2>
    <%= simple_form_for [@tool, @reservation] do |f| %>
      <%= f.input  :start_date,  required: false, input_html: {id: "range_start"} %>
      <%= f.input  :return_date, required: false, input_html: {id: "range_end"} %>
      <%= f.button :submit, "Book", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>